    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "uuid": "^7.0.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },

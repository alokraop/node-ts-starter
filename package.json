{
  "name": "ts-starter",
  "version": "1.0.0",
  "private": true,
  "main": "bin/index.js",
  "scripts": {
    "compile": "tsc",
    "clean": "rimraf dist/*",
    "build": "npm run clean && npm run compile",
    "ts-node": "ts-node",
    "test:unit": "jest src --coverage --passWithNoTests",
    "test:e2e": "jest -c jest.e2e.config.ts --runInBand test/ --coverage",
